<?xml version="1.0"?>
<launch>
    <arg name="filename" default="RealTrajectory.csv"/>
    
    <node name="Arduino_node" pkg="rosserial_python" type="serial_node.py">
        <param name="port" value="/dev/ttyACM0"/>
    </node>
    
    <node name="Localization" pkg="two_wheel" type="Odometry.py"/>
    <node name="TrajectoryRecorder" pkg="two_wheel" type="TrajectoryRecorder.py">
        <param name="csv_path" value="$(find two_wheel)/csv/$(arg filename)"/>
    </node>

    <node name="NewCmdPublisher" pkg="two_wheel" type="RLTargetPublisher.py"/>
    <node name="Controler" pkg="two_wheel" type="TrajectoryTrackingControler.py" output="screen"/>
</launch>